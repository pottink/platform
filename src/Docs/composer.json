{
    "name": "shopware/docs",
    "description": "Docs for the Shopware Core",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shopware\\Docs\\": ""
        },
        "exclude-from-classmap": [
            "*/Test/"
        ]
    },
    "config": {
        "platform": {
            "php": "7.4.0"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.4.x-dev",
            "dev-trunk": "6.4.x-dev"
        }
    },
    "require": {
        "cocur/slugify": "4.0.0",
        "erusev/parsedown-extra": "0.8.1",
        "guzzlehttp/guzzle": "~7.2",
        "shopware/core": "*",
        "symfony/config": "~5.2.3",
        "symfony/console": "~5.2.3",
        "symfony/dependency-injection": "~5.2.3",
        "symfony/filesystem": "~5.2.3",
        "symfony/finder": "~5.2.3",
        "symfony/http-kernel": "~5.2.3"
    }
}
