        }
    },
    "require": {
        "shopware/core": "*",
        "symfony/framework-bundle": "~5.2.3",
        "symfony/http-foundation": "~5.2.3",
        "symfony/routing": "~5.2.3",
        "symfony/mime": "~5.2.3"
    }
}
