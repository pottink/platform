{
    "name": "shopware/administration",
    "description": "Administration frontend for the Shopware Core",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shopware\\Administration\\": ""
        }
    },
    "config": {
        "platform": {
            "php": "7.4.0"
        },
        "sort-packages": true
